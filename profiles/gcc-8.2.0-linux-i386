[build_requires]
binutils/2.31@includeos/tools
[settings]
os=Linux
os_build=Linux
arch=x86
arch_build=x86_64
compiler=gcc
compiler.version=8
compiler.libcxx=libstdc++11
cppstd=17
build_type=Release
[options]
[env]
CC=/usr/bin/gcc-8
CXX=/usr/bin/g++-8
CFLAGS=-m32 -msse3 -mfpmath=sse
CXXFLAGS=-m32 -msse3 -mfpmath=sse
